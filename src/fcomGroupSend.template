# Template for FCOM group send
record( bo, "$(LAST)ALL_SEND" )
{
	field( SCAN, "$(SCAN=Passive)" )
	field( EVNT, "$(EC=0)" )
	field( TSE,  "$(EC=40)" )
	field( DTYP, "FCOM" )
	field( ZNAM, "Wait" )
	field( ONAM, "Send" )
	field( HIGH, "0.0001" )
	field( OUT,  "#C0S$(GROUP)@SEND" )
	field( FLNK, "$(FLNK=)" )
	info( autosaveFields, "OSV ZSV DESC" )
}
